package com.baotoan.not_safe;

public class Logger {
	private static Logger INSTANCE;
	
	private Logger() {}
	
	public static Logger getInstance() {
		if(INSTANCE == null) {
			INSTANCE = new Logger();
		}
		
		return INSTANCE;
	}
	
	public void debug(String message) {
		System.out.println("DEBUG: " + message);
	}
}
