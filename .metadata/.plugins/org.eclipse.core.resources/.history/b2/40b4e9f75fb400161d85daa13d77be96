	public FileTrace(boolean debug, String path) {
		super(debug);
		this.printWriter = new PrintWriter(new FileWriter(new File(path)));
	}

	@Override
	public void debug(String message) {
		i
	}

	@Override
	public void error(String message) {
		
	}

}
